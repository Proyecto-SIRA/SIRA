@for (int i = 0; i < ViewBag.Autores.Count; i++)
{
    <tr>
        <td>
            @ViewBag.Autores[i].Nombre
        </td>
        <td>
            @ViewBag.Autores[i].PrimerApellido
        </td>
        <td>
            @ViewBag.Autores[i].Email
        </td>
        <td>
            @ViewBag.Autores[i].Porcentaje
        </td>
    </tr>
}

